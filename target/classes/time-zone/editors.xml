<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" url="http://www.mulesoft.org/schema/mule/time-zone" prefix="time-zone">
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="TimeZone" description="" icon="" localId="parentConfig" image="" abstract="true" extends="http://www.mulesoft.org/schema/mule/time-zone/abstractTime-zoneConnectorGeneric"></global-cloud-connector>
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="TimeZone: Configuration" description="Global Configuration configuration information." icon="icons/theme.classic/time-zone-connector-small.png" localId="config" image="icons/theme.classic/time-zone-connector-large.png" abstract="false" extends="http://www.mulesoft.org/schema/mule/time-zone/parentConfig" connectivityTesting="off" metaData="static" metaDataKeyDisplay="DISPLAY_NAME" supportsUserDefinedMetaData="false">
    <attribute-category caption="General" description="Time-zone configuration properties.">
      <group id="time-zoneGenericProperties" caption="Basic Settings">
        <name name="name" caption="Name" description="Give a name to this configuration so it can be later referenced by config-ref." required="true"></name>
      </group>
      <group id="general" caption="General">
        <string defaultValue="Hello" name="greeting" caption="Greeting" description="Greeting message." supportsExpressions="false" required="false" javaType="java.lang.String"></string>
        <string defaultValue="How are you?" name="reply" caption="Reply" description="Reply message." supportsExpressions="false" required="false" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </global-cloud-connector>
  <cloud-connector caption="Abstracttime-zoneconnectorgeneric" description="Interact with Time-zone." icon="icons/theme.classic/time-zone-connector-small.png" localId="abstractTime-zoneConnectorGeneric" image="icons/theme.classic/time-zone-connector-large.png" abstract="true">
    <attribute-category caption="General" description="General.">
      <group id="abstractTime-zoneConnectorGeneric" caption="Basic Settings">
        <globalRef requiredType="http://www.mulesoft.org/schema/mule/time-zone/config" name="config-ref" caption="Connector Configuration" description="Specify which configuration to use for this invocation." required="true"></globalRef>
      </group>
    </attribute-category>
  </cloud-connector>
