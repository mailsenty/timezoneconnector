          <mode modeId="http://www.mulesoft.org/schema/mule/time-zone/gettimezone" modeLabel="Gettimezone"></mode>
        </modeSwitch>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector returnType="java.lang.String" caption="Gettimezone" icon="icons/theme.classic/time-zone-connector-small.png" localId="gettimezone" image="icons/theme.classic/time-zone-connector-large.png" abstract="true" extends="http://www.mulesoft.org/schema/mule/time-zone/abstractTime-zoneConnectorGeneric" metaData="static">
    <attribute-category caption="General" description="General.">
      <group id="general" caption="General">
        <string name="latitude" caption="Latitude" description="" required="true" javaType="java.lang.String"></string>
        <string name="longitude" caption="Longitude" description="" required="true" javaType="java.lang.String"></string>
